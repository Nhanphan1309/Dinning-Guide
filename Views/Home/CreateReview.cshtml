@using System.Data.Entity;
@using System.Linq;
@model Dinning_Guide.Models.Rate.Rate
@{
    ViewBag.Title = "CreateReview";
}
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title> CreateReview </title>
</head>
<body>
    <div>
        <form action=@Url.Action("CreateReview","Home") method="post">

            Rate: <input type="text" id="TxtRate" name="Rate" value="" /><br />
            Review: <input type="text" id="TxtReview" name="Review" value="" /><br />
            <input type="submit" name="BtnSave" value="Save Review" />
            <input type="button" name="BtnReset" value="Reset" />
        </form>
    </div>
</body>
</html>